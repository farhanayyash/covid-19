<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>getCountryResult</title>
</head>
<body>
  <header>
    <a href="/">Home</a>
    <a href="/MyRecords">MyRecords</a>
    <a href="/AllCountries">All Countries</a>
  </header>
  <% data.forEach(element => { %>
    <div>
      <h1>Country: <%= element.Country %></h1>
      <h1>Cases: <%= element.Cases %></h1>
      <h1>Date: <%= element.Date %></h1>
    </div>
    <hr>
  <% }); %>
  
</body>
</html>